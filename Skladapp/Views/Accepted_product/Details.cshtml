@model Skladapp.Models.Accepted_product

@{
    ViewBag.Title = "Details";
}

<h2>Информация</h2>

<div>
    <h4>Принятые товары</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Количество
        </dt>

        <dd>
            @Html.DisplayFor(model => model.quantity)
        </dd>

        <dt>
            ID_отправки
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Dispatch.ID_dispatch)
        </dd>

        <dt>
            Наименование ассортимента
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Ranges.Names_range)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Редактирование", "Edit", new { id = Model.ID }) |
    @Html.ActionLink("Вернуться обратно", "Index")
</p>
