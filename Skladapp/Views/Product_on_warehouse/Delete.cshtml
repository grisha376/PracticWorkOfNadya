@model Skladapp.Models.Product_on_warehouse

@{
    ViewBag.Title = "Delete";
}

<h2>Удаление</h2>

<h3>Вы уверенны что хотите удалить это?</h3>
<div>
    <h4>Товары на складе</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Количество
        </dt>

        <dd>
            @Html.DisplayFor(model => model.quantity)
        </dd>

        <dt>
           Наименование ассортимента
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Ranges.Names_range)
        </dd>

        <dt>
            Наименование склада
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Warehouse.Names)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Удалить" class="btn btn-default" /> |
            @Html.ActionLink("Вернуться обратно", "Index")
        </div>
    }
</div>
